{
    "cmake.ignoreCMakeListsMissing": true,
    "spellright.language": [
        "English (Australian)"
    ],
    "spellright.documentTypes": [
        "markdown",
        "latex",
        "plaintext"
    ],
    "spellright.parserByClass": {
        "Log": {
            "parser": "markdown"
        }
    },
    "python.languageServer": "None",
    "files.associations": {
        "*.log": "jsonc",
        "*.lalrpop": "rust",
        "*.tera": "django-html",
        "*.json": "jsonc",
        "*.html": "html",
        "*.lock": "plaintext",
        "*.mdx": "markdown",
        "*.ungrammar": "ungrammar",
        "stdio.h": "c"
    }
}
